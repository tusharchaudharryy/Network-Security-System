# OpenAI
OPENAI_API_KEY="your_openai_api_key_here"

# MongoDB 
MONGO_CONNECTION_STRING="mongodb+srv://username:password@cluster0.mongodb.net/?retryWrites=true&w=majority"
MONGO_DB_NAME="farm_ai_insights"
MONGO_INSIGHTS_COLLECTION="insights"
MONGO_RUNS_COLLECTION="analysis_runs"

# AWS S3 
AWS_ACCESS_KEY_ID="your_aws_access_key"
AWS_SECRET_ACCESS_KEY="your_aws_secret_key"
S3_BUCKET_NAME="your-s3-bucket-name"
AWS_REGION="your-aws-region"

# Set to "true" to enable the MobileNetV2 filter
USE_AI_FILTER=true
# How many frames per second to scan
FILTER_FPS=.20
# Max frames to save (safety cap)
MAX_SAVED_FRAMES=30
# AI Relevance Confidence Threshold
AI_RELEVANCE_CONFIDENCE=0.7